<%= form_for @file_upload, :html => {:multipart => true} do |f| %>
<%= f.hidden_field :file_cache %>
<fieldset>
  <%= render 'errors/form', :object => @file_upload %>
  <div class="field">
    <label>
      file:
    </label>
    <%= f.file_field :file %>
  </div>
  <div class="field">
    <label>
      filename (optional):
    </label>
    <%= f.text_field :filename %>
  </div>
  <div class="actions">
    <%= f.submit :class => "continue" %>
  </div>
</fieldset>
<% end %>
