<!DOCTYPE html>
<html>
  <head>
    <title>aweman - awe management</title>
    <%= stylesheet_link_tag :all %>
    <%= javascript_include_tag :defaults %>
    <%= csrf_meta_tag %>
  </head>
  <body>
    <div id="header">
      <h1>awe<span>man</span> <small>awe management</small></h1>
      <p id="header_menu">
        <%= link_to "Users", users_path %>
        <%= link_to "Groups", groups_path %>
        <%= link_to "Projects", projects_path %>
        <%= link_to "Clients", clients_path %>
      </p>
    </div>
    <div id="content">
      <%= yield %>
      <%= debug(params) if Rails.env.development? %>
    </div>
	<div id="footer">by msquare and allspark</div>
  </body>
</html>
